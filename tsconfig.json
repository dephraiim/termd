
{
  "compilerOptions": {
    "target": "ES2015",
    "module": "commonjs",
    "outDir": "dist",
    "moduleResolution": "node",
    "lib": [
        "ES2015",
        "DOM",
        "ES6"
    ],
    "strict": true,
    "typeRoots": [
        "@types",
        "./node_modules/@types"
    ],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
